programa
{
    funcao inicio()
    {
        real a, b, c

       
        escreva("Digite o valor de a: ")
        leia(a)
        
        escreva("Digite o valor de b: ")
        leia(b)
        
        escreva("Digite o valor de c: ")
        leia(c)

       
        escreva("Valor de a: ", a, "\n")
        
        escreva("Valor de b: ", b, "\n")
        
        escreva("Valor de c: ", c, "\n")
        
    }
}
