programa
{
    funcao inicio()
    {
        real a, b, c
        real delta
        escreva("Digite o valor de a: ")
        leia(a)
        escreva("Digite o valor de b: ")
        leia(b)
        escreva("Digite o valor de c: ")
        leia(c)

        delta = b * b - 4 * a * c
        
        escreva("Valor de a: ", a, "\n")
        escreva("Valor de b: ", b, "\n")
        escreva("Valor de c: ", c, "\n")

        escreva("Valor de delta: ", delta, "\n")
    }
}
